const int LM35 = A0;
float   temperatura;


void setup (){
  Serial.begin(9600);
}

void loop ()
{
   temperatura = (float(analogRead(LM35))*5/(1023))/0.01;

   Serial.println("--- Sensor de Temperatura - Sidinho ---");
   Serial.print("Temperatura: ");
   Serial.println(temperatura);
   delayÂ (1000);

}
